import java.io.*;
import java.util.*;

public class StaticInitializer {
    static{
    	Scanner sc = new Scanner(System.in);
    	int B = sc.nextInt();
    	int H = sc.nextInt();
 
    	try{
    		if (B <= 0 || H <= 0){
    			throw new Exception("java.lang.Exception: Breadth and height must be positive");
    			}
    		else {
    			System.out.println(B * H);
    		}
    		}

    		catch(Exception e)
    		{
    			System.out.println(e);
    		}
    }


}
