import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class EndOfFile {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        ArrayList al = new ArrayList();
        while (sc.hasNext()){
            al.add(sc.nextLine());
        }
        
        for(int i=0;i<al.size();i++){

            System.out.println((i+1) + " " + al.get(i));
        }
        sc.close();
    }
}